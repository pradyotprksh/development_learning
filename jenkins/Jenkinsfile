pipeline {
    agent any

    stages {
        stage('Parallel Build Android Project') {
            steps {
                echo 'Checking out android projects'
            }

            parallel {
                stage('JenkinsLearning project') {
                    steps {
                        dir('./android/JenkinsLearning') {
                            script {
                                python3 ./android_script.py
                            }
                        }
                    }
                }
            }
        }
    }
}
